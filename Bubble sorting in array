#include <stdio.h>
int main() {
 int i,j,a[20],n,temp;
 printf("enter how many number you want:");
  scanf("%d",&n);
   for(i=0;i<n;i++){
      scanf("%d",&a[i]);
  }
for(i=0;i<n;i++){
    for(j=i+1;j<n;j++){
        if(a[i]>a[j]){
            temp=a[i];
            a[i]=a[j];
            a[j]=temp;
        }
}
}
for(i=0;i<n;i++){
     printf("%d\n",a[i]);
  }
}
