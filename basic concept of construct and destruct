// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int c=0;
class stu{
    int age;
    public:
    stu(){
        c++;
        cout<<"NO OF OBJECTS CREATED:"<<c<<endl;
    }
   
    ~stu(){
        cout<<"the object destroyed is"<<c<<endl;
        c--;
    }
};

int main() {
    stu s1,s2,s3;
    return 0;
}
