// Online C compiler to run C program online
#include <stdio.h>
int main() {
   int arr[]={10,20,30,40,50},size;
   size=sizeof(arr)/sizeof(arr[0]);
int i,j,temp,sorted;
    for(i=0;i<size;i++){
        sorted=0;
    for(j=0;j<size-1;j++){
            if(arr[j]>arr[j+1]){
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
                sorted=1;
            }
        }
        
    }
    if(sorted==0){
        printf("already sorted\n");
    }
    for(i=0;i<size;i++){
        printf("%d\n",arr[i]);
    }
    return 0;
}
