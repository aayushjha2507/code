#include <stdio.h>
int main() {
 int a[20];
  int i,n;
 printf("enter how many elements of arrray");
 scanf("%d",&n);
 for(i=0;i<n;i++){
     scanf("%d",&a[i]);
 }
//   for(i=0;i<n;i++){
//      printf("%d\n",a[i]);
//  }
 int pos;
 printf("now enter the position delete in an array");
 scanf("%d",&pos);
 for(i=pos-1;i<n;i++){
     a[i]=a[i+1];
 }
 n=n-1;
   for(i=0;i<n;i++){
     printf("%d\n",a[i]);
 }
 }
